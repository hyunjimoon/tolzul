properties:
  file.name:
    displayName: Paper
  thesisPaper:
    displayName: Thesis
  thesisChapter:
    displayName: Chapter
  rank:
    displayName: ‚≠ê
views:
  - type: table
    name: Thesis Papers
    filters:
      and:
        - file.inFolder("Space/Library/1ÎÖºÎ¨∏Ïö©")
        - file.name.startsWith("üìú")
    order:
