{UTF-8}
CumErr  = A FUNCTION OF( incerr)
	~	
	~		|

incerr=A FUNCTION OF(incerr,Actual Orders,Orders Placed) ~~|
incerr=
	Actual Orders-Orders Placed)^2/(0.24^2)
	~	
	~		|

R Squared=
	1-ZIDZ( "Sum of Residuals^2" , Count*Sy*Sy)
	~	Dimensionless
	~	Coefficient of determination. (1-Sum of Squared errors/Total Sum of \
		Squares)
	|

MAE over Mean=
	ZIDZ(ZIDZ(Sum of AE ,Count),M Y )
	~	Dimensionless
	~	Mean Absolute Error as a fraction of the mean of the data
	|

Y=
	Actual Orders
	~	cases
	~	The simulated data series
	|

X=
	Orders Placed
	~	cases
	~	The historical data input
	|

Weight of Payoff Function=
	1/SD of Residuals
	~	1/cases
	~	Weight assigned to the payoff function. For each data point, weight is \
		multiplied with the discrepancy between simulated result and the \
		historical data, then this product is squared. These values are summed up \
		for all data points and subtracted from 0 to find the payoff. Using \
		optimization, the payoff value is maximized.
	|

SD of Residuals=
	2.414
	~	cases
	~	Standard deviation of the residuals of 48 weeks.
	|

Orders Placed=
	INTEGER(iOrders+0.5 )
	~	cases
	~	Indicated orders are rounded off to the nearest integer in order to obtain \
		integer values for orders placed. INTEGER function truncates the decimal \
		part of its input. 0.5 is added to the equation for rounding off the input \
		to the nearest integer rather than just truncating the decimal part.
	|

"Sum of Residuals^2" =
	MSE *Count
	~	cases*cases
	~	Sum of squared errors.
	|

Start Time=
	1
	~	Week
	~	Date of first data point
	|

pick=
	STEP(1,Start Time)*(1-STEP(1,FINAL TIME + TIME STEP/2))*IF THEN ELSE(Time/Interval= \
		INTEGER(Time/Interval),1 , 0 )
	~	Dimensionless
	~	Takes a value of one for every data point available, assuming the data are \
		available at intervals of Interval between the Start Time and End Time.
	|

S Residuals =
	SQRT("M Residuals^2" -(M Residuals *M Residuals ))
	~	cases
	~	Standard Deviation of errors. Calculated using the 'hand computation' \
		formula to calculate the standard deviation without prior knowledge of the \
		mean. Sterman (1984), pg. 64
	|

"M Residuals^2" =
	ZIDZ("Sum of Residuals^2" ,Count)
	~	cases*cases
	~	Mean of Residuals^2 (Sum Residuals^2/n).
	|

Sum of Residuals =
	Sum of Past Residuals +Residuals
	~	cases
	~	Sum of errors.
	|

Sum of Past Residuals = INTEG (
	Residuals /dt,
		0)
	~	cases
	~	Sum of errors that precede the current value.
	|

M Residuals =
	ZIDZ(Sum of Residuals ,Count)
	~	cases
	~	Mean of Errors (sum residuals/n).
	|

Sum of Xi =
	Sum of Past Xi +Xi
	~	cases
	~	Sum of x's (simulated).
	|

Interval=
	1
	~	Week
	~	Interval between data points
	|

Count=
	Count for Past Points+pick
	~	Dimensionless
	~	Counter for # of points.
	|

Count for Past Points= INTEG (
	pick/dt,
		0)
	~	Dimensionless
	~	Counter for # of previous points.
	|

dif cov =
	2*Sx *Sy *(1-r )
	~	cases*cases
	~	Difference of covariances
	|

dif mea =
	(M X -M Y )*(M X -M Y )
	~	cases*cases
	~	Difference of Means (bias)
	|

dif var =
	(Sx -Sy )*(Sx -Sy )
	~	cases*cases
	~	Difference of variances
	|

Sum of Past AE = INTEG (
	ABS(Xi  - Yi )/dt,
		0)
	~	cases
	~	Sum of Absolute Errors for previous points.
	|

dt=
	TIME STEP
	~	Week
	~		|

"Sum of Past X^2" = INTEG (
	Xi *Xi /dt,
		0)
	~	cases*cases
	~	Sum of x^2 for previous points.
	|

Sum of Past Xi = INTEG (
	Xi /dt,
		0)
	~	cases
	~	Sum of x's (simulated) that precede the current value.
	|

Sum of Past XY = INTEG (
	Xi *Yi /dt,
		0)
	~	cases*cases
	~	Sum of x*y for previous points.
	|

"Sum of Past Y^2" = INTEG (
	Yi *Yi /dt,
		0)
	~	cases*cases
	~	Sum of y^2 for previous points.
	|

Sum of Past Yi = INTEG (
	Yi /dt,
		0)
	~	cases
	~	Sum of y's that precede the current value.
	|

"Sum of X^2" =
	"Sum of Past X^2" +(Xi *Xi )
	~	cases*cases
	~	Sum of x^2.
	|

RMSE =
	SQRT(MSE )
	~	cases
	~	Root Mean Square Error
	|

Yi =
	pick*Y
	~	cases
	~	Sampled simulated variable
	|

M X =
	ZIDZ(Sum of Xi ,Count)
	~	cases
	~	Mean of x (sum x)/n
	|

M Y =
	ZIDZ(Sum of Yi ,Count)
	~	cases
	~	Mean of y (sum y)/n
	|

MAPE =
	ZIDZ(Sum of APE ,Count)
	~	Dimensionless
	~	Mean Absolute Percent Error
	|

MSE =
	dif mea  + dif var  + dif cov
	~	cases*cases
	~	Mean Square Error. The addition of the three components
	|

MX2 =
	ZIDZ("Sum of X^2" ,Count)
	~	cases*cases
	~	Mean of x^2 (sum x^2)/n
	|

Mxy =
	ZIDZ(Sum of XY ,Count)
	~	cases*cases
	~	Mean of x*y (sum x*y)/n
	|

MY2 =
	ZIDZ("Sum of Y^2" ,Count)
	~	cases*cases
	~	Mean of y^2 (sum y^2)/n
	|

r =
	ZIDZ(Mxy -(M X *M Y ),Sx *Sy )
	~	Dimensionless
	~	Correlation coefficient. Calculated through the 'hand computation' formula.
		   Sterman (1984) pg. 63
	|

"r^2" =
	r *r
	~	Dimensionless
	~	Correlation coefficient squared
	|

Residuals=
	Yi-Xi
	~	cases
	~	Errors (discrepancy between actual data points and simulated data points).
	|

Sy =
	SQRT(MY2 -(M Y *M Y ))
	~	cases
	~	Standard Deviation of y. Calculated using the 'hand computation'
		formula
		   to calculate the standard deviation without prior knowledge of
		the mean.
		   Sterman (1984), pg. 64
	|

Sum of XY =
	Sum of Past XY +(Xi *Yi )
	~	cases*cases
	~	Sum of x*y.
	|

Sum of Yi =
	Sum of Past Yi +Yi
	~	cases
	~	Sum of y's.
	|

Sum of AE =
	Sum of Past AE +ABS(Xi  - Yi )
	~	cases
	~	Sum of Absolute Errors.
	|

Sum of APE=
	Sum of Past APE +ABS(ZIDZ(Xi -Yi ,Yi ))
	~	Dimensionless
	~	Sum of Absolute Percent Errors.
	|

Um =
	ZIDZ(dif mea ,MSE )
	~	Dimensionless
	~	Bias inequality proportion
	|

Sum of Past APE= INTEG (
	ABS(ZIDZ(Xi -Yi ,Yi ))/dt,
		0)
	~	Dimensionless
	~	Sum of Absolute Percent Errors for previous points.
	|

Uc =
	ZIDZ(dif cov ,MSE )
	~	Dimensionless
	~	Covariance inequality proportion
	|

Xi =
	pick*X
	~	cases
	~	The historic data series
	|

Us =
	ZIDZ(dif var ,MSE )
	~	Dimensionless
	~	Variance inequality proportion
	|

"Sum of Y^2" =
	"Sum of Past Y^2" +(Yi *Yi )
	~	cases*cases
	~	Sum of y^2.
	|

Sx =
	SQRT(MX2 -(M X *M X ))
	~	cases
	~	Standard Deviation of x. Calculated using the 'hand computation'
		formula
		   to calculate the standard deviation without prior knowledge of
		the mean.
		   Sterman (1984), pg. 64
	|

Change in Expected Orders =
	Theta *(Incoming Orders -Expected Orders )
	~	cases/Week
	~	The rate of change in the forecast is proportional to the gap between \
		expected orders and incoming orders.
	|

Actual Orders
	~	cases
	~	Actual orders placed by the players.
	|

Discrepancy =
	"S-Prime" -Effective Inventory -(Fraction of Supply Line Taken into Account *Supply Line\
		 )
	~	cases
	~	Discrepancy between desired inventory and current inventory added to a \
		certain fraction of supply line discrepancy.
	|

Effective Inventory
	~	cases
	~	Amount of current inventory or backlog (whichever is existent).
	|

iOrders=
	MAX(0,Expected Orders +Fraction of Discrepancy Ordered Each Period *Discrepancy )
	~	cases
	~	Orders determined by the decision rule.
	|

Expected Orders = INTEG (
	Change in Expected Orders ,
		4)
	~	cases
	~	Forecasted amount of orders for the future. Historical information of the \
		incoming orders is updated by the player according to the orders placed by \
		the customer.
	|

Forecast Adjustment Time =
	IF THEN ELSE(Theta =0, 1e+12, 1/Theta )
	~	Week
	~	The forecast adjustment time constant is the reciprocal of the parameter \
		Theta estimated in the original analysis (Sterman 1989). If Theta is \
		assigned the value 0, 1/Theta would be infinite, so the if then else \
		statement is used for preventing computational errors.
	|

Fraction of Discrepancy Ordered Each Period =
	0.5
	~	Dmnl [0,1]
	~	Fraction of the discrepancy between desired inventory and current \
		inventory that is ordered each period.
	|

Fraction of Supply Line Taken into Account =
	0.5
	~	Dmnl [0,2]
	~	Fraction of supply line discrepancy ordered each period.
	|

Incoming Orders
	~	cases
	~	Orders received by the player.
	|

"S-Prime" =
	20
	~	cases [0,?]
	~	Value of S-prime is considered constant. It is formulated as 'Desired \
		Inventory' + 'Desired Supply Line' * 'Fraction of Supply Line Taken into \
		Account' (which are all assumed to be constant)
	|

Supply Line
	~	cases
	~	Units which have been ordered but not yet received.
	|

Theta =
	0.5
	~	1/Week [0,1]
	~	Fractional adjustment rate of the player's forecast for incoming orders.
	|

********************************************************
	.Control
********************************************************~
		Simulation Control Parameters
	|

FINAL TIME  = 48
	~	Week
	~	The final time for the simulation.
	|

INITIAL TIME  = 1
	~	Week
	~	The initial time for the simulation.
	|

SAVEPER  = 1
	~	Week [0,?]
	~	The frequency with which output is stored.
	|

TIME STEP  = 1
	~	Week [0,?]
	~	The time step for the simulation.
	|

