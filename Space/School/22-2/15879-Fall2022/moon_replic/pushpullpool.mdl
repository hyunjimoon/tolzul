{UTF-8}
Adopters= INTEG (
	first demand-breakaway-reuse,
		1)
	~	people
	~		|

Advertisement effect ratio=
	0.5
	~	Dmnl
	~		|

breakaway=
	Adopters * Breakaway ratio / Duration time
	~	people/day
	~		|

Breakaway ratio=
	0.01
	~	Dmnl
	~	0.7 shows oscillation
	|

Duration time=
	1
	~	day
	~		|

first demand=
	Max(0, Potential adopters * Advertisement effect ratio  + Adopters * Word of mouth effect ratio\
		 *  Potential adopters / Total population)
	~	people/day
	~		|

Leavers= INTEG (
	breakaway,
		0)
	~	people
	~		|

Potential adopters= INTEG (
	-first demand,
		Total population - Adopters - Leavers
		)
	~	people
	~		|

Promotion effect ratio=
	0.01
	~	Dmnl
	~		|

Recommendation effect ratio=
	0.1
	~	Dmnl
	~		|

reuse=
	Adopters * Satisfaction ratio / Duration time
	~	people/day
	~		|

Satisfaction ratio=
	0.9
	~	Dmnl
	~		|

total demad=
	first demand + reuse
	~	people/day
	~		|

Total population=
	40000
	~	people
	~		|

Word of mouth effect ratio=
	Satisfaction ratio * Recommendation effect ratio + Promotion effect ratio
	~	Dmnl
	~		|

********************************************************
	.Control
********************************************************~
		Simulation Control Parameters
	|

FINAL TIME  = 100
	~	day
	~	The final time for the simulation.
	|

INITIAL TIME  = 0
	~	day
	~	The initial time for the simulation.
	|

SAVEPER  = 
        TIME STEP 
	~	day [0,?]
	~	The frequency with which output is stored.
	|

TIME STEP  = 1
	~	day [0,?]
	~	The time step for the simulation.
	|

