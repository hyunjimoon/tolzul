views:
  - type: table
    name: All Operations Projects
    columns:
      - property: file
        label: "Project"
      - property: journal_target
        label: "Journal"
      - property: tools.최적손
        label: "최적손%"
      - property: tools.확률눈
        label: "확률눈%"
      - property: tools.통계귀
        label: "통계귀%"
      - property: status
        label: "Status"
    filters:
      and:
        - 'market == "🐢Operations"'
    sort:
      - column: tools.최적손
        direction: DESC
        
  - type: table
    name: Active Projects
    columns:
      - property: file
        label: "Project"
      - property: journal_target
        label: "Journal"
      - property: first_principle
        label: "Principle"
    filters:
      and:
        - 'market == "🐢Operations"'
        - or:
            - 'status == "active"'
            - 'status == "ongoing"'
    sort:
      - column: file.mtime
        direction: DESC
        
  - type: table
    name: By Journal
    columns:
      - property: file
        label: "Project"
      - property: tools.최적손
        label: "최적손%"
      - property: status
        label: "Status"
    filters:
      and:
        - 'market == "🐢Operations"'
    sort:
      - column: journal_target
        direction: ASC
    group:
      - property: journal_target
