views:
  - type: table
    name: Dots Folders
    columns:
      - property: file
        label: "Folder"
      - property: file.folder
        label: "Path"
    filters:
      and:
        - 'file.folder.contains("Space/Dots")'
        - '!file.folder.endsWith("Space/Dots")'
    sort:
      - column: file.name
        direction: ASC
    limit: 10
