id: papers-inv-cards
type: card
filter: (collection contains Papers) and (file.name starts with 📜) and (field contains 🐢inv)
sort:
  - property: rank
    direction: desc
  - property: year
    direction: desc
formulas:
  Untitled: ""
views:
  - type: table
    name: Table
    filters:
      and:
        - field == ["🐢inv"]
    order:
      - file.name
      - field
      - formula.Untitled
